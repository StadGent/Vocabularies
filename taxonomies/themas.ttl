@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix gentthema: <http://stad.gent/id/ns/themas/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://stad.gent/id/ns/themas/> a skos:ConceptScheme;
                                   skos:prefLabel "Thema's Stad Gent"@nl.

gentthema:burgerzaken a owl:Class, skos:Concept;
                      skos:prefLabel "Burgerzaken"@nl;
                      rdfs:comment "Documenten en administratieve zaken die belangrijk zijn in het leven van elke burger."@nl;
                      skos:inScheme <http://stad.gent/id/ns/themas/>;
                      skos:broadMatch <http://data.gov.be/en/taxonomy/term/41>, <http://publications.europa.eu/resource/authority/data-theme/GOVE>.

gentthema:cultuur-sport-vrije-tijd a owl:Class, skos:Concept;
                                   skos:prefLabel "Cultuur, Sport & Vrije Tijd"@nl;
                                   rdfs:comment "Ook bibliotheek, jeugd, kunst, erfgoed, evenementen, toerisme en media."@nl;
                                   skos:inScheme <http://stad.gent/id/ns/themas/>;
                                   skos:exactMatch <http://data.gov.be/en/taxonomy/term/59>, <http://publications.europa.eu/resource/authority/data-theme/EDUC>;
                                   skos:narrowMatch <http://ns.thedatatank.com/dcat/themes#CulturalEvents>, <http://ns.thedatatank.com/dcat/themes#CulturalHeritage>, <http://ns.thedatatank.com/dcat/themes#Media>, <http://ns.thedatatank.com/dcat/themes#Sport>, <http://ns.thedatatank.com/dcat/themes#Tourism>.

gentthema:mobiliteit-openbare-werken a owl:Class, skos:Concept;
                                     skos:prefLabel "Mobiliteit & openbare werken"@nl;
                                     rdfs:comment "Ontdek hier de infrastructuur- en mobiliteitsprojecten in uw buurt en onze plannen om het in de toekomst nog beter te maken."@nl;
                                     skos:inScheme <http://stad.gent/id/ns/themas/>;
                                     skos:closeMatch <http://ns.thedatatank.com/dcat/themes#Mobility>;
                                     skos:broadMatch <http://data.gov.be/en/taxonomy/term/36>, <http://publications.europa.eu/resource/authority/data-theme/TRAN>.

gentthema:onderwijs-kinderopvang a owl:Class, skos:Concept;
                                 skos:prefLabel "Onderwijs & kinderopvang"@nl;
                                 rdfs:comment "Gent is de grootste onderwijsstad van Vlaanderen. Met een ruim aanbod voor kleuters, kinderen, tieners en studenten."@nl;
                                 skos:inScheme <http://stad.gent/id/ns/themas/>;
                                 skos:narrowMatch <http://ns.thedatatank.com/dcat/themes#Education>;
                                 skos:broadMatch <http://data.gov.be/en/taxonomy/term/33>, <http://publications.europa.eu/resource/authority/data-theme/EDUC>.

gentthema:werken-ondernemen a owl:Class, skos:Concept;
                            skos:prefLabel "Werken & Ondernemen"@nl;
                            rdfs:comment "Wilt u werken of stage lopen in Gent? Of hebt u vragen over het ondernemerschap?"@nl;
                            skos:inScheme <http://stad.gent/id/ns/themas/>;
                            skos:closeMatch <http://ns.thedatatank.com/dcat/themes#Economy>;
                            skos:narrowMatch <http://ns.thedatatank.com/dcat/themes#Financing>, <http://ns.thedatatank.com/dcat/themes#Work>;
                            skos:broadMatch <http://data.gov.be/en/taxonomy/term/38>, <http://publications.europa.eu/resource/authority/data-theme/ECON>.

gentthema:natuur-milieu a owl:Class, skos:Concept;
                        skos:prefLabel "Natuur & Milieu"@nl;
                        rdfs:comment "Samen maken we van Gent een groene en duurzame stad"@nl;
                        skos:inScheme <http://stad.gent/id/ns/themas/>;
                        skos:broadMatch <http://data.gov.be/en/taxonomy/term/34>, <http://publications.europa.eu/resource/authority/data-theme/ENVI>.

gentthema:wonen-bouwen a owl:Class, skos:Concept;
                       skos:prefLabel "Wonen & (ver)bouwen"@nl;
                       rdfs:comment "Kwaliteitsvol, energiezuinig en betaalbaar wonen in Gent, stedenbouwkundige vergunning, verkavelingsvergunning"@nl;
                       skos:inScheme <http://stad.gent/id/ns/themas/>;
                       skos:broadMatch <http://data.gov.be/en/taxonomy/term/39>, <http://publications.europa.eu/resource/authority/data-theme/SOCI>.

gentthema:welzijn-gezondheid a owl:Class, skos:Concept;
                             skos:prefLabel "Welzijn & Gezondheid"@nl;
                             rdfs:comment "Wegwijs in gezondheids- en welzijnszorg, OCMW, armoede, integratie en migratie, senioren, personen met een beperking, vrijwilligerswerk, Digitaal.Talent@Gent"@nl;
                             skos:inScheme <http://stad.gent/id/ns/themas/>;
                             skos:closeMatch <http://ns.thedatatank.com/dcat/themes#Environment>, <http://ns.thedatatank.com/dcat/themes#LivingHealthWelfare>;
                             skos:narrowMatch <http://ns.thedatatank.com/dcat/themes#SafetyAndJustice>;
                             skos:broadMatch <http://data.gov.be/en/taxonomy/term/40>, <http://data.gov.be/en/taxonomy/term/39>, <http://publications.europa.eu/resource/authority/data-theme/HEAL>, <http://publications.europa.eu/resource/authority/data-theme/SOCI>.

gentthema:stad-beleid-bestuur a owl:Class, skos:Concept;
                              skos:prefLabel "Stad, beleid en bestuur"@nl;
                              rdfs:comment  "alles over de troeven van de Stad Gent, op welke manier het bestuur de stad doet leven en hoe u zelf uw steentje kan bijdragen."@nl;
                              skos:inScheme <http://stad.gent/id/ns/themas/>;
                              skos:broadMatch <http://data.gov.be/en/taxonomy/term/42> ;
                              skos:narrowMatch <http://ns.thedatatank.com/dcat/themes#Demography>, <http://ns.thedatatank.com/dcat/themes#Financing>, <http://ns.thedatatank.com/dcat/themes#Law>, <http://ns.thedatatank.com/dcat/themes#Research>, <http://ns.thedatatank.com/dcat/themes#SafetyAndJustice>;
                              skos:closeMatch <http://publications.europa.eu/resource/authority/data-theme/GOVE>, <http://ns.thedatatank.com/dcat/themes#Government>;
                              skos:narrowMatch <http://publications.europa.eu/resource/authority/data-theme/REGI>;
